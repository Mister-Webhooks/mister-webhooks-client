{
  "name": "@mister-webhooks/client",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier . --check --ignore-path ../.prettierignore",
    "format:write": "prettier . --write --ignore-path ../.prettierignore",
    "lint": "eslint . --ext .ts",
    "version": "pnpm version --no-git-tag-version"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "tsup": "^8.5.0",
    "typescript": "latest"
  },
  "dependencies": {
    "avsc": "^5.7.7",
    "cbor2": "^2.0.0",
    "kafkajs": "^2.2.4"
  }
}
